<tr id="<%= dom_id friend %>">
  <td>
    <%= friend.first_name %>
  </td>

  <td>
    <%= friend.last_name %>
  </td>

  <td>
    <%= friend.email %>
  </td>

  <td>
    <%= friend.phone %>
  </td>

  <td>
    <%= friend.twitter %>
  </td>

  <td><%= link_to "Show", friend, class:"btn btn-dark" %> </td>

  <td><%= link_to "Edit", edit_friend_path(friend), class:"btn btn-secondary" %></td>

  <td><%= button_to 'Delete', friend, method: :delete, class:"btn btn-danger" %></td>
</tr>
